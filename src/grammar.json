{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "fernlog",
  "rules": {
    "document": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_block"
      }
    },
    "_block": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "heading"
        },
        {
          "type": "SYMBOL",
          "name": "_blank_line"
        }
      ]
    },
    "_text": {
      "type": "PATTERN",
      "value": "[^\\n]"
    },
    "_whitespace": {
      "type": "PATTERN",
      "value": "[ \\t]+"
    },
    "_blank_line": {
      "type": "PATTERN",
      "value": "\\s*\\n"
    },
    "emphasis": {
      "type": "STRING",
      "value": "!"
    },
    "question": {
      "type": "STRING",
      "value": "?"
    },
    "todo": {
      "type": "STRING",
      "value": "●"
    },
    "done": {
      "type": "STRING",
      "value": "✔"
    },
    "cancelled": {
      "type": "STRING",
      "value": "✖"
    },
    "moved": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "<"
        },
        {
          "type": "STRING",
          "value": ">"
        }
      ]
    },
    "evnt": {
      "type": "STRING",
      "value": "○"
    },
    "note": {
      "type": "STRING",
      "value": "-"
    },
    "feel": {
      "type": "STRING",
      "value": "❤"
    },
    "h1_marker": {
      "type": "PATTERN",
      "value": "#[^#]"
    },
    "h2_marker": {
      "type": "PATTERN",
      "value": "##[^#]"
    },
    "h3_marker": {
      "type": "PATTERN",
      "value": "###[^#]"
    },
    "h4_marker": {
      "type": "PATTERN",
      "value": "####[^#]"
    },
    "h5_marker": {
      "type": "PATTERN",
      "value": "#####[^#]"
    },
    "h6_marker": {
      "type": "PATTERN",
      "value": "######"
    },
    "header_content": {
      "type": "PATTERN",
      "value": "[^\\r\\n]+"
    },
    "heading": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "h1"
        },
        {
          "type": "SYMBOL",
          "name": "h2"
        },
        {
          "type": "SYMBOL",
          "name": "h3"
        },
        {
          "type": "SYMBOL",
          "name": "h4"
        },
        {
          "type": "SYMBOL",
          "name": "h5"
        },
        {
          "type": "SYMBOL",
          "name": "h6"
        }
      ]
    },
    "h1": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "marker",
          "content": {
            "type": "SYMBOL",
            "name": "h1_marker"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "content",
              "content": {
                "type": "SYMBOL",
                "name": "header_content"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_heading_content"
          }
        }
      ]
    },
    "h2": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "marker",
          "content": {
            "type": "SYMBOL",
            "name": "h2_marker"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "content",
              "content": {
                "type": "SYMBOL",
                "name": "header_content"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_heading_content"
          }
        }
      ]
    },
    "h3": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "marker",
          "content": {
            "type": "SYMBOL",
            "name": "h3_marker"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "content",
              "content": {
                "type": "SYMBOL",
                "name": "header_content"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_heading_content"
          }
        }
      ]
    },
    "h4": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "marker",
          "content": {
            "type": "SYMBOL",
            "name": "h4_marker"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "content",
              "content": {
                "type": "SYMBOL",
                "name": "header_content"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_heading_content"
          }
        }
      ]
    },
    "h5": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "marker",
          "content": {
            "type": "SYMBOL",
            "name": "h5_marker"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "content",
              "content": {
                "type": "SYMBOL",
                "name": "header_content"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_heading_content"
          }
        }
      ]
    },
    "h6": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "marker",
          "content": {
            "type": "SYMBOL",
            "name": "h6_marker"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "content",
              "content": {
                "type": "SYMBOL",
                "name": "header_content"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_heading_content"
          }
        }
      ]
    },
    "_heading_content": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "entry"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "entry": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "mark",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "REPEAT1",
                  "content": {
                    "type": "CHOICE",
                    "members": [
                      {
                        "type": "SYMBOL",
                        "name": "emphasis"
                      },
                      {
                        "type": "SYMBOL",
                        "name": "question"
                      }
                    ]
                  }
                },
                {
                  "type": "BLANK"
                }
              ]
            }
          },
          {
            "type": "FIELD",
            "name": "type",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "note"
                },
                {
                  "type": "SYMBOL",
                  "name": "todo"
                },
                {
                  "type": "SYMBOL",
                  "name": "feel"
                },
                {
                  "type": "SYMBOL",
                  "name": "evnt"
                },
                {
                  "type": "SYMBOL",
                  "name": "done"
                },
                {
                  "type": "SYMBOL",
                  "name": "moved"
                },
                {
                  "type": "SYMBOL",
                  "name": "cancelled"
                }
              ]
            }
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "_text"
            }
          },
          {
            "type": "STRING",
            "value": "\n"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "REPEAT",
                "content": {
                  "type": "SEQ",
                  "members": [
                    {
                      "type": "SYMBOL",
                      "name": "_whitespace"
                    },
                    {
                      "type": "SYMBOL",
                      "name": "entry"
                    }
                  ]
                }
              },
              {
                "type": "BLANK"
              }
            ]
          }
        ]
      }
    }
  },
  "extras": [
    {
      "type": "STRING",
      "value": "\r"
    },
    {
      "type": "STRING",
      "value": "\n"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}