{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "fern",
      "camelcase": "Fern",
      "title": "Fern",
      "scope": "source.fern",
      "file-types": [
        "fern"
      ],
      "injection-regex": "^fern$",
      "class-name": "TreeSitterFern"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Markdown-like parser for the Fern journal file",
    "authors": [
      {
        "name": "Andie Keller",
        "email": "andie@bugwhisperer.dev",
        "url": "https://bugwhisperer.dev/"
      }
    ],
    "links": {
      "repository": "https://git.sr.ht/~bugwhisperer/tree-sitter-fern"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}